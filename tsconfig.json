{
  "extends": "astro/tsconfigs/strictest",
  "compilerOptions": {
    "strict": true,
    "strictNullChecks": true,
    "checkJs": false,
    "baseUrl": ".",
    "paths": {
      "@layouts/*": [
        "src/layouts/*"
      ],
      "@assets/*": [
        "src/assets/*"
      ],
      "@data/*": [
        "src/data/*"
      ],
      "@styles/*": [
        "src/styles/*"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@utils/*": [
        "src/utils/*"
      ],
      "@i18n/*": [
        "src/i18n/*"
      ],
      "@/*": [
        "./src/*"
      ]
      
    },
    "incremental": true,
    "skipLibCheck": true,
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}